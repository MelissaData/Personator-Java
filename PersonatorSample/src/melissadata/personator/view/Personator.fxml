                                    <font>
                                        <Font size="18.0" />
                                    </font>
                                </Label>
                                <ToolBar layoutX="26.0" layoutY="41.0" orientation="VERTICAL" prefHeight="98.0" prefWidth="81.0" AnchorPane.leftAnchor="26.0" AnchorPane.topAnchor="41.0">
